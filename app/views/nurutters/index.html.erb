<p id="notice"><%= notice %></p>

<h1>ぬるったー</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nurutters.each do |nurutter| %>
      <tr>
        <td><%= nurutter.content %></td>
        <td><button type="button" class="btn btn-primary"><%= link_to '見る', nurutter %></button></td>
        <td><button type="button" class="btn btn-success"><%= link_to '編集する', edit_nurutter_path(nurutter) %></button></td>
        <td><buutton type="button" class="btn btn-danger"><%= link_to '削除する', nurutter, method: :delete, data: { confirm: 'Are you sure?' } %></button></td>
      </tr>
    <% end %>
  </tbody>
</table>

